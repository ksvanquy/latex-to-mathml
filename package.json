{
  "name": "latex-to-mathml",
  "version": "1.0.0",
  "description": "A TypeScript library to convert LaTeX mathematical expressions to MathML 3.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "latex",
    "mathml",
    "mathematics",
    "math",
    "converter",
    "parser",
    "typescript",
    "w3c",
    "mathematical-markup-language"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/latex-to-mathml.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/latex-to-mathml/issues"
  },
  "homepage": "https://github.com/yourusername/latex-to-mathml#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "xmlbuilder": "^15.0.0"
  }
}
